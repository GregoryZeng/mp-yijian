.page {
  padding: 0 20rpx 0;
}
.scrollable_view {
  width: auto;
  height: 1170rpx;
  margin: 20rpx auto 10rpx;
  border: 5rpx solid #000000;
  border-radius: 30rpx;
  overflow: hidden;
  -webkit-transform: translateY(0);
          transform: translateY(0);
}
.scrollable_view .noticebar {
  transition: all 1s;
  max-height: 100rpx;
  position: relative;
}
.scrollable_view .noticebar.transition {
  opacity: 0;
  -webkit-transform: translateX(-100px);
          transform: translateX(-100px);
  margin: 0;
  max-height: 0;
}
.scrollable_view .noticebar .noticebar_img {
  display: block;
  margin: 0 auto 0;
  height: 70rpx;
}
.scrollable_view .noticebar .noticebar_text {
  position: absolute;
  width: 50%;
  bottom: 0;
  left: 120rpx;
}
.scrollable_view .template_grid {
  display: grid;
  grid-template-columns: minmax(0, 1fr) minmax(0, 1fr);
  -webkit-column-gap: 10rpx;
          column-gap: 10rpx;
  row-gap: 20rpx;
}
.scrollable_view .template_grid .template_item .template_thumbnail {
  max-width: 100%;
}
.scrollable_view .template_grid .template_item .template_desc {
  display: block;
  text-align: center;
  position: relative;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: contain;
  padding: 16rpx 0 16rpx;
  background-position: center;
}
.scrollable_view .scrollable_bottom {
  padding-top: 60rpx;
  padding-bottom: 40rpx;
  text-align: center;
}
.tabbar {
  display: flex;
}
.tabbar .tabbar_elem {
  display: inline-block;
  flex-basis: 0;
  flex-grow: 1;
  text-align: center;
  font-size: 40rpx;
  padding: 16rpx 0 16rpx 10rpx;
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center;
  background-image: url(data:image/png;base64,);
}
.tabbar .tabbar_elem.tabbar_elem_selected {
  background-image: url(data:image/png;base64,);
}

